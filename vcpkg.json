
{
  "name": "photobroom",
  "version-string": "1.7.0",
  "port-version": 1,
  "homepage": "https://github.com/Kicer86/photobroom",
  "description": "Photo Broom is a tool for managing your photos.",
  "dependencies": [
    {
      "name": "dlib",
      "default-features": false,
      "features": [ "fftw3" ]
    },
    {
      "name": "opencv4",
      "default-features": false,
      "features": [ "contrib" ]
    },
    {
      "name": "ffmpeg",
      "default-features": false,
      "features": [ "avcodec", "avformat" ]
    },
    "boost-multi-index",
    "exiv2",
    "jsoncpp",
    "libguarded",
    "libwebp",
    "qtbase",
    "qtsvg",
    {
      "name": "qttools",
      "default-features": false,
      "features": [ "linguist", "qml" ]
    },
    "qtdeclarative",
    "qtmultimedia",
    "qtquick3d",
    "breakpad"
  ]
}
